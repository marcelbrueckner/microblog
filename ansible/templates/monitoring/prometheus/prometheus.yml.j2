global:
  scrape_interval: 5s
  evaluation_interval: 5s
  external_labels:
      monitor: 'microblog'
rule_files:
scrape_configs:
  - job_name: 'node-exporter'
    static_configs:
      - targets: ['{{ app_ec2_public_dns }}:9100']
  - job_name: 'microblog-app'
    static_configs:
      - targets: ['{{ app_ec2_public_dns }}:80']
